/**
 * 16-way Or gate.  out = in[0] or in[1] or ... or in[7]
 */
 
CHIP Or16Way {

    IN  in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7],out=or1);
    Or8Way(in=in[8..15],out=or2);

    Or(a=or1,b=or2,out=out);
}
